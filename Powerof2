#include <bits/stdc++.h>
using namespace std;

bool isapowerof2(int n)
{
  int x=n-1;
  
  if(n==0)
  return false;
  
  if(n & x == 0)
  return true;
  
  return false;
}

int main()
{
int n;
cout<<"Enter the number"<<endl;
cin>>n;

if(isapowerof2(n))
cout<<"Power of 2"<<endl;
else
cout<<"Not a power of 2"<<endl;

return 0;
}
